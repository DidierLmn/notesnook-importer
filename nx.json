{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/build", "{projectRoot}/dist"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build:test"]
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json"
}
